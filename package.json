{
  "name": "kupler",
  "version": "0.0.1",
  "type": "module",
  "description": "Local only package for linking globals such as React to avoid conflict.",
  "files": [
    "bin",
    "fixtures",
    "prepost.cjs"
  ],
  "bin": {
    "kup": "bin/cli.js",
    "kupler": "bin/cli.js"
  },
  "scripts": {
    "commit-pre": "node ./prepost.cjs pre",
    "commit-post": "node ./prepost.cjs post",
    "postinstall": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Blujedi LLC <blujedicorp@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/blujedis/kupler#readme",
  "bugs": {
    "url": "https://github.com/blujedis/kupler/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blujedis/kupler.git"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "ansi-colors": "^4.1.1",
    "eslint": "^7.26.0",
    "global-dirs": "^3.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "log-symbols": "^5.0.0",
    "npm-check-updates": "^11.8.1",
    "table": "^6.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run commit-pre && lint-staged",
      "post-commit": "npm run commit-post",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}